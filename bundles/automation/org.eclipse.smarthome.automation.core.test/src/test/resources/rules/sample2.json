{
    "enabled": true,
    "id": "sample2",
    "name": "Toggle lamp on/off every 10 Seconds",
    "on": [
        {
            "type": "timer",
            "parameters": {
                "cron": "0/10 * * * * ?"
            }
        }
    ],
    "then": [
        {
            "type": "toggle",
            "parameters": {
                "itemName": "lamp2"
            }
        }
    ]
}